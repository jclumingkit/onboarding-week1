generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Food {
  id          String   @id @default(uuid())
  name        String
  description String
  image       String
  rating      Int
  isPublic    Boolean  @default(true)
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
}